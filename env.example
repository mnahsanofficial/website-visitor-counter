# Website Visitor Counter Environment Configuration
# Copy this file to .env and fill in your values

# Railway Backend (Default)
RAILWAY_API_BASE=https://your-railway-app.up.railway.app

# Vercel Backend
VERCEL_API_BASE=https://your-vercel-app.vercel.app

# Netlify Backend
NETLIFY_API_BASE=https://your-netlify-app.netlify.app

# Cloudflare Backend
CLOUDFLARE_API_BASE=https://your-cloudflare-app.pages.dev

# Custom Backend
CUSTOM_API_BASE=https://your-custom-backend.com

# Rate Limiting (Optional)
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=10

# Security (Optional)
ENABLE_RATE_LIMITING=true
ENABLE_IP_FINGERPRINTING=true
